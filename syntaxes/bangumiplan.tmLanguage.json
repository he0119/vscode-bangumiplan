{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "BangumiPlan",
	"patterns": [
		{
			"include": "#bgm-id"
		}
	],
	"repository": {
		"bgm-id": {
			"patterns": [
				{
					"name": "entity.name.function.bgm-id.bangumiplan",
					"match": "\\[\\d+\\]",
					"captures": {
						"0": {
							"name": "markup.underline.link.bgm-id.bangumiplan"
						}
					}
				}
			]
		}
	},
	"scopeName": "source.bangumiplan"
}
